{
  "name": "@globalart/zod-to-proto",
  "version": "1.0.18",
  "author": {
    "name": "GlobalArt, Inc"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GlobalArtInc/ecosystem.git#main"
  },
  "keywords": [
    "zod",
    "protobuf",
    "grpc",
    "protobuf-schema",
    "protobuf-generator"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest --runInBand --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "coveralls": "yarn run test:cov --coverageReporters=text-lcov | coveralls",
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "prepublishOnly": "npm run build",
    "publish:dev": "npm publish --access public --tag dev",
    "publish:npm": "release-it --config ../../.release-it.json"
  },
  "description": "Convert Zod schemas to Protobuf definitions",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/passport": "^1.0.17",
    "prettier": "^3.6.2",
    "release-it": "19.0.6",
    "ts-node": "^10.9.2",
    "tsdown": "0.17.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "inflection": "3.0.2",
    "zod": "4.1.13"
  },
  "publishConfig": {
    "access": "public"
  }
}
