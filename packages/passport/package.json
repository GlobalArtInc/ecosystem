{
  "name": "@globalart/passport",
  "version": "1.1.3",
  "author": {
    "name": "GlobalArt, Inc"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest --runInBand --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "coveralls": "yarn run test:cov --coverageReporters=text-lcov | coveralls",
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "prepublishOnly": "npm run build",
    "publish:dev": "npm publish --access public --tag dev",
    "publish:npm": "release-it --config ../../.release-it.json"
  },
  "keywords": [
    "openid-connect",
    "oauth2",
    "authentication",
    "globalart"
  ],
  "description": "OpenID Connect strategy for GlobalArt SSO integration",
  "devDependencies": {
    "@types/express": "catalog:dev",
    "@types/node": "catalog:dev",
    "@types/passport": "catalog:dev",
    "prettier": "catalog:dev",
    "release-it": "catalog:dev",
    "ts-node": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev"
  },
  "dependencies": {
    "express": "catalog:prod",
    "node-fetch": "catalog:prod",
    "openid-connect": "catalog:prod",
    "passport": "catalog:prod",
    "url": "catalog:prod"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {},
  "optionalDependencies": {}
}